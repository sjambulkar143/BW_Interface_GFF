<?xml version="1.0" encoding="utf-8"?>
<xs:schema
	xmlns="http://www.sita.aero/ams6-xml-api-datatypes"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	targetNamespace="http://www.sita.aero/ams6-xml-api-datatypes"
	elementFormDefault="qualified">

	<xs:simpleType name="ApiVersion">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d+\.\d+"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ApiRequestId">
		<xs:restriction base="xs:string" />
	</xs:simpleType>

  <xs:simpleType name="User">
    <xs:restriction base="xs:string" />
  </xs:simpleType>

  <xs:simpleType name="Timestamp">
    <xs:restriction base="xs:dateTime" />
  </xs:simpleType>

	<xs:simpleType name="ResultStatus">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Success"/>
			<xs:enumeration value="Failure"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="ErrorInformation">
		<xs:sequence>
			<xs:element name="ErrorCode" type="xs:string"/>
			<xs:element name="ErrorDescription" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ErrorCollection">
		<xs:sequence>
			<xs:element name="Error" type="ErrorInformation" minOccurs="1" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="ApiRequest">
		<xs:sequence>
			<xs:element name="RequestId" type="ApiRequestId" minOccurs="0" />
			<xs:element name="Token" type="LoginToken" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ApiResponse">
		<xs:sequence>
			<xs:element name="RequestId" type="ApiRequestId" minOccurs="0" />      
			<xs:element name="Status" type="ResultStatus" />
			<xs:element name="Data" type="ResultDataTypes" minOccurs="0" />
			<xs:element name="Errors" type="ErrorCollection" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="CodeContext">
		<xs:restriction base="xs:string">
			<xs:enumeration value="ICAO"/>
			<xs:enumeration value="IATA"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="FlightKind">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Arrival"/>
			<xs:enumeration value="Departure"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CustomFieldType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Boolean"/>
			<xs:enumeration value="Double"/>
			<xs:enumeration value="Integer"/>
			<xs:enumeration value="String"/>
			<xs:enumeration value="DateTime"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CustomFieldLookupValue">
		<xs:restriction base="xs:string" />
	</xs:simpleType>

	<xs:simpleType name="DOOP">
		<xs:restriction base="xs:string">
			<xs:pattern value="[.1][.2][.3][.4][.5][.6][.7]" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="TimeOfDay">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-2][0-9]:[0-5][0-9]"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="CustomFieldLookupGroup">
		<xs:sequence>
			<xs:element name="LookupValue" type="CustomFieldLookupValue" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomFieldInformation">
		<xs:sequence>
			<xs:element name="CustomFieldType" type="CustomFieldType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DefaultValue" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="DisplayName" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="ExternalReference" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="IsSoftLookup" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
			<xs:element name="LookupGroup" type="CustomFieldLookupGroup" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomFieldsCollection">
		<xs:sequence>
			<xs:element name="CustomField" type="CustomFieldInformation" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>


	<xs:complexType name="LookupCode">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="codeContext" type="CodeContext" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="FlightId">
		<xs:sequence>
			<xs:element name="FlightKind" type="FlightKind"/>
			<xs:element name="AirlineDesignator" type="LookupCode" minOccurs="1" maxOccurs="unbounded"/>
			<xs:element name="FlightNumber" type="xs:string"/>
			<xs:element name="ScheduledDate" type="xs:date"/>
			<xs:element name="AirportCode" type="LookupCode" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="LinkedFlightInformation">
		<xs:sequence>
			<xs:element name="FlightId" type="FlightId"/>
			<xs:element name="Value" type="PropertyValue" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ScheduledFlightsId">
		<xs:sequence>
			<xs:element name="FlightKind" type="FlightKind"/>
			<xs:element name="AirlineDesignator" type="LookupCode" minOccurs="1" maxOccurs="unbounded"/>
			<xs:element name="FlightNumber" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PropertyValue">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="propertyName" type="xs:string" use="required"/>
				<xs:attribute name="codeContext" type="CodeContext" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="TablePropertyValue">
		<xs:sequence>
			<xs:element name="Row" type="TableRow" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="propertyName" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="TableValueUpdate">
		<xs:sequence>
			<xs:element name="Row" type="TableRow" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="propertyName" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="TableRow">
		<xs:sequence>
			<xs:element name="Value" type="PropertyValue" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="propertyName" type="xs:string" use="optional"/>
	</xs:complexType>

	<xs:complexType name="AircraftTypeId">
		<xs:sequence>
			<xs:element name="AircraftTypeCode" type="LookupCode" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AircraftType">
		<xs:sequence>
			<xs:element name="AircraftTypeId" type="AircraftTypeId"/>
			<xs:element name="Value" type="PropertyValue" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AircraftId">
		<xs:sequence>
			<xs:element name="Registration" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Aircraft">
		<xs:sequence>
			<xs:element name="AircraftId" type="AircraftId"/>
			<xs:element name="Value" type="PropertyValue" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AirportId">
		<xs:sequence>
			<xs:element name="AirportCode" type="LookupCode" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AirlineId">
		<xs:sequence>
			<xs:element name="AirlineCode" type="LookupCode" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="RouteViaPoint">
		<xs:sequence>
			<xs:element name="AirportCode" type="LookupCode" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="sequenceNumber" type="xs:int" use="required"/>
	</xs:complexType>

	<xs:complexType name="Route">
		<xs:sequence>
			<xs:element name="ViaPoints">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="RouteViaPoint" type="RouteViaPoint" minOccurs="1" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="customsType" type="xs:string" use="optional"/>
	</xs:complexType>

	<xs:complexType name="RouteId">
		<xs:sequence>
			<xs:element name="ViaPoints">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="RouteViaPoint" type="RouteViaPoint" minOccurs="1" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PropertyChange">
		<xs:sequence>
			<xs:element name="OldValue" type="xs:string" />
			<xs:element name="NewValue" type="xs:string" />
		</xs:sequence>
		<xs:attribute name="propertyName" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="TableValueChange">
		<xs:sequence>
			<xs:element name="OldValue" type="TablePropertyValue" />
			<xs:element name="NewValue" type="TablePropertyValue" />
		</xs:sequence>
		<xs:attribute name="propertyName" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="ScheduledTimeChange">
