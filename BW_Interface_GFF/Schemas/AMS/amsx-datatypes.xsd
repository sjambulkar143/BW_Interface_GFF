	<xs:complexType name="CheckInResource">
		<xs:complexContent>
			<xs:extension base="Resource"></xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CheckInResourceCollection">
		<xs:sequence>
			<xs:element name="CheckIn" type="CheckInResource" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
