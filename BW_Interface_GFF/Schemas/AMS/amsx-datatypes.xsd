			<xs:element name="OldValue" type="xs:dateTime" />
			<xs:element name="NewValue" type="xs:dateTime" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AircraftTypeChange">
		<xs:sequence>
			<xs:element name="OldValue">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AircraftType" type="AircraftType" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="NewValue">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AircraftType" type="AircraftType" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AircraftChange">
		<xs:sequence>
			<xs:element name="OldValue">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Aircraft" type="Aircraft" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="NewValue">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Aircraft" type="Aircraft" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="RouteChange">
		<xs:sequence>
			<xs:element name="OldValue">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Route" type="Route" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="NewValue">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Route" type="Route" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="LinkedFlightChange">
		<xs:sequence>
			<xs:element name="OldValue">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="LinkedFlight" type="LinkedFlightInformation" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="NewValue">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="LinkedFlight" type="LinkedFlightInformation" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomsType">
		<xs:sequence>
			<xs:element name="CustomsTypeId" type="CustomsTypeId"/>
			<xs:element name="Value" type="PropertyValue" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomsTypeId">
		<xs:sequence>
			<xs:element name="CustomsTypeCode" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomsTypeChange">
		<xs:sequence>
			<xs:element name="OldValue">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CustomsType" type="CustomsType" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="NewValue">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CustomsType" type="CustomsType" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Activity">
		<xs:sequence>
			<xs:element name="Value" type="PropertyValue" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="code" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="ActivityChange">
		<xs:sequence>
			<xs:element name="Change" type="PropertyChange" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="code" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="ActivityAdded">
		<xs:attribute name="code" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="ActivityRemoved">
		<xs:attribute name="code" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="ActivityUpdate">
		<xs:sequence>
			<xs:element name="Update" type="PropertyValue" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="code" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="Event">
		<xs:sequence>
			<xs:element name="Value" type="PropertyValue" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="code" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="EventChange">
		<xs:sequence>
			<xs:element name="Change" type="PropertyChange" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="code" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="EventAdded">
		<xs:attribute name="code" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="EventRemoved">
		<xs:attribute name="code" type="xs:string" use="required"/>
	</xs:complexType>

	<xs:complexType name="EventUpdate">
		<xs:sequence>
			<xs:element name="Update" type="PropertyValue" maxOccurs="unbounded" />
		</xs:sequence>
		<xs:attribute name="code" type="xs:string" use="required"/>
	</xs:complexType>


	<xs:complexType name="Area">
		<xs:sequence>
			<xs:element name="Value" type="PropertyValue" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Resource">
		<xs:sequence>
			<xs:element name="Value" type="PropertyValue" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Area" type="Area" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Slot">
